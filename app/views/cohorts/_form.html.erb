    <div id="edit-box-1">
      <%= form_with(model: cohort, local: true) do |form| %>
    <h3>Cohort Info:</h3>
      <div class="field">
        <%= form.label :name %>
        <%= form.text_field :name %>
      </div>

      <div class="field">
        Subject<br />
        <%= form.collection_select(:course_id, Course.all, :id, :name) %>
      </div>

      <div class="field">
        <%= form.label :start %>
        <%= form.date_select :start %>
      </div>

      <div class="field">
        <%= form.label :end %>
        <%= form.date_select :end %>
      </div>

      <div class="actions">
        <%= form.submit %>
      </div>
    <% end %>
  </div>
  
  <%= yield :edit %>

<br />


